{
  "name": "omen-subgraph",
  "version": "1.0.0",
  "description": "Subgraph for Omen",
  "main": "index.js",
  "scripts": {
    "migrate": "truffle migrate --all --reset",
    "create-local": "graph create --node http://localhost:8020/ cag/omen",
    "remove-local": "graph remove --node http://localhost:8020/ cag/omen",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 cag/omen",
    "codegen": "truffle exec render-templates.js && graph codegen",
    "build": "graph build",
    "bootstrap-test": "wait-on tcp:8545 && npm run migrate && wait-on http-get://localhost:8000 && npm run create-local",
    "test-fresh-graph": "npm run codegen && wait-on tcp:5001 && npm run deploy-local && npm run test-existing-graph",
    "test-existing-graph": "mocha",
    "test": "npm run bootstrap-test && npm run test-fresh-graph"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cag/omen-subgraph.git"
  },
  "author": "Gnosis (https://gnosis.io)",
  "license": "LGPL-3.0",
  "bugs": {
    "url": "https://github.com/cag/omen-subgraph/issues"
  },
  "homepage": "https://github.com/cag/omen-subgraph#readme",
  "devDependencies": {
    "@gnosis.pm/conditional-tokens-contracts": "^1.0.1",
    "@gnosis.pm/conditional-tokens-market-makers": "^1.6.0",
    "@graphprotocol/graph-cli": "^0.17.1",
    "@graphprotocol/graph-ts": "^0.17.0",
    "@realitio/realitio-contracts": "github:cag/realitio-contracts",
    "@truffle/contract": "^4.1.7",
    "axios": "^0.19.2",
    "delay": "^4.3.0",
    "fs-extra": "^8.1.0",
    "mocha": "^7.0.1",
    "mocha-steps": "^1.3.0",
    "mustache": "^4.0.0",
    "realitio-gnosis-proxy": "github:fvictorio/realitio-gnosis-proxy",
    "should": "^13.2.3",
    "truffle": "^5.1.10",
    "wait-on": "^4.0.0",
    "web3": "^1.2.5"
  }
}
